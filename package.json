{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:device": "yarn ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && pod deintegrate && pod cache clean --all && pod install  && cd ../",
    "check:codepush": "appcenter codepush deployment list --app bepyan/boda-ios -k && appcenter codepush deployment list --app bepyan/boda-aos -k",
    "check:release": "appcenter codepush deployment list -a bepyan/boda-ios && appcenter codepush deployment list -a bepyan/boda-aos",
    "release:staging": "appcenter codepush release-react -a bepyan/boda-ios -d Staging && appcenter codepush release-react -a bepyan/boda-aos -d Staging",
    "release:production": "appcenter codepush release-react -a bepyan/boda-ios -d Production && appcenter codepush release-react -a bepyan/boda-aos -d Production"
  },
  "dependencies": {
    "@notifee/react-native": "^4.1.0",
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/netinfo": "^8.0.0",
    "@react-native-firebase/analytics": "^14.5.0",
    "@react-native-firebase/app": "^14.5.0",
    "@react-native-firebase/messaging": "^14.5.0",
    "@react-navigation/bottom-tabs": "^6.0.7",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "appcenter": "4.4.3",
    "appcenter-analytics": "4.4.3",
    "appcenter-crashes": "4.4.3",
    "axios": "^0.25.0",
    "react": "17.0.2",
    "react-hook-form": "^7.27.0",
    "react-native": "0.67.2",
    "react-native-code-push": "^7.0.4",
    "react-native-dotenv": "^3.2.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "2.1.1",
    "react-native-haptic-feedback": "^1.13.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-query": "^3.34.14",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/react": "^17",
    "@types/react-native": "^0.66.15",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.68.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
